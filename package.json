{
  "name": "life-server",
  "version": "1.1.0",
  "author": "Marcos Marrero",
  "description": "Servidor SERPICA CANARIAS",
  "main": "./dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./types/index.d.ts"
    }
  },
  "types": "/lib/typescript.d.ts",
  "scripts": {
    "lint": "eslint ./src/**/*",
    "dev": "npx tsc && node --watch ./dist/index.js",
    "start": "node dist/src/index.js",
    "start:dev": "node --watch --loader ts-node/esm ./src/index.ts",
    "start:dist": "npx tsc && node dist/index.js",
    "build": "rimraf dist && tsc && copyfiles -u 1 ./src/**/*.graphql dist/src",
    "build:dev": "yarn run clean && tsc",
    "build:watch": "tsc -w",
    "clean": "rimraf dist",
    "generate": "graphql-codegen --config codegen.ts",
    "watch": "node --watch --trace-warnings --loader ts-node/esm ./src/index.ts",
    "prettier-format": "prettier --config .prettierrc ./src/**/*.ts --write",
    "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "ordersVerify": "tsc && node dist/ordersVerify.js"
  },
  "keywords": [],
  "license": "ISC",
  "eslintConfig": {
    "parserOptions": {
      "project": "./ts.config.json"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@tsconfig/node16": "16.1.0",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/express-session": "1.17.7",
    "@types/flat": "5.0.2",
    "@types/glob": "8.1.0",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "20.5.1",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "6.4.9",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "@types/styled-components": "5.1.26",
    "@types/styled-system": "5.1.16",
    "@types/validator": "13.11.9",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "copyfiles": "^2.4.1",
    "cross-env": "7.0.3",
    "eslint": "8.0.1",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-n": "15.0.0 || 16.0.0 ",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.33.1",
    "graphql-codegen": "^0.4.0",
    "lint-staged": "14.0.1",
    "onchange": "7.1.0",
    "prettier": "3.0.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "2.0.0",
    "tslib": "2.6.2",
    "types": "^0.1.1",
    "typescript": "^5.4.5",
    "volar-service-vetur": "0.0.17"
  },
  "peerDependencies": {
    "adminjs": "7.8.1"
  },
  "dependencies": {
    "@adminjs/design-system": "4.0.2",
    "@adminjs/express": "6.0.0",
    "@adminjs/mongoose": "4.1.0",
    "@adminjs/themes": "1.0.1",
    "@graphql-tools/load-files": "7.0.0",
    "@graphql-tools/schema": "10.0.3",
    "@handlebars/allow-prototype-access": "^1.0.5",
    "@types/nodemailer-express-handlebars": "^4.0.5",
    "adminjs": "7.8.1",
    "argon2": "0.30.3",
    "axios": "1.4.0",
    "cloudinary": "2.2.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-formidable": "1.2.0",
    "express-handlebars": "^7.1.2",
    "express-session": "1.17.3",
    "glob": "10.3.14",
    "graphql": "16.8.1",
    "graphql-middleware": "6.1.35",
    "graphql-type-json": "0.3.2",
    "graphql-yoga": "5.3.1",
    "jsonwebtoken": "9.0.2",
    "mongoose": "8.3.4",
    "node-cron": "^3.0.3",
    "nodemailer": "6.9.13",
    "nodemailer-express-handlebars": "^6.1.2",
    "react-feather": "2.0.10",
    "react-redux": "8.1.1",
    "rxjs": "7.8.1",
    "validator": "13.11.0",
    "winston": "3.10.0"
  },
  "engines": {
    "node": ">=20.5.1 || 18.16.0"
  }
}
